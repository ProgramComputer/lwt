{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "includePattern": "src/js/.+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "templates/default", 
        "encoding": "utf8",          
        "destination": "./docs/js/", 
        "recurse": false,
        "readme": "./README.md"
    }
}

